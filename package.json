{
  "name": "bare",
  "version": "0.0.1",
  "description": "Single page application skeleton for production and development in 2015 & 2016",
  "homepage": "https://github.com/meanJim/bare#readme",
  "license": "MIT",
  "scripts": {
    "start": "npm run link; node server",
    "link": "rm -f ./node_modules/components; rm -f ./node_modules/common; ln -s ../client/components ./node_modules/components; ln -s ../client/common ./node_modules/common;",
    "production": "rm -rf ./dist; npm run link; NODE_ENV=production webpack --config webpack.production.config.js;",
    "deploy": "rm -rf ./dist; npm run link; npm run production; PORT=3000 NODE_ENV=production node server;"
  },
  "keywords": [
    "preact",
    "less",
    "postcss",
    "webpack",
    "babel",
    "skeleton codebase",
    "seed project",
    "starter project",
    "demo"
  ],
  "bugs": {
    "url": "https://github.com/meanJim/bare/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meanJim/bare.git"
  },
  "author": {
    "email": "jimmy-lee@live.com",
    "name": "Jimmy Lee",
    "url": "http://jimmyl.ee"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "crypto": "^0.0.3",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.3.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.19.0",
    "http-proxy": "^1.13.3",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "path": "^0.12.7",
    "postcss-loader": "^0.9.1",
    "style-loader": "^0.13.1",
